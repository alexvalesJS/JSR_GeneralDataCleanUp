        <Field Name="Contact_s_Organization__c">
          <DataField>Contact_s_Organization__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ContactId">
          <DataField>ContactId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select opp.Id
, opp.Opp_Name__c
, opp.Cluster__c
, opp.Description__c
, opp.Status__c
, opp.Opportunity_Type__c
, con.Name
, ntpart.Contact_s_Organization__c
, con.Id "ContactId"
from NT_Opportunity__c opp
left outer join NT_Opp_Participant__c ntpart on opp.Id = ntpart.NT_Opportunity__c
left outer join contact con on ntpart.Contact__c = con.Id
where convert(varchar,opp.Cluster__c) like '%Energy Enterprise%'
order by Opp_Name__c</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
