  <DataSets>
    <DataSet Name="OrgParam">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select acc.id, acc.name
from nt_opportunity__c ntopp
inner join NT_Opp_Participant__c op on ntopp.Id = op.NT_Opportunity__c
inner join Contact c on op.contact__c = c.id
inner join Account acc on c.accountid = acc.id
union select acc.id, acc.name
from NT_Press_Co_To_Promote__c pctp
inner join Account acc on pctp.Organization_Being_Promoted__c = acc.id
order by acc.name</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="NTOpportunity">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Org">
            <Value>=Parameters!Org.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct acc.id "OrgId"
, acc.name "OrgName"
, ntopp.ID "OppId"
, convert(varchar,ntopp.cluster__c) "NTCluster"
, ntopp.Date_Entered__c
, ntopp.name "NTOppName"
, ntopp.Opp_Name__c "OppName"
, ntopp.status__c
, ntopp.Opportunity_Type__c
, convert(varchar(1000),ntopp.Description__c) "Desc"
, convert(varchar,ntopp.NT_Staff_Engaged__c) "NTStaff"
from nt_opportunity__c ntopp
inner join NT_Opp_Participant__c op on ntopp.Id = op.NT_Opportunity__c
inner join Contact c on op.contact__c = c.id
inner join Account acc on c.accountid = acc.id
where acc.Id = @Org</CommandText>
