        <CommandText>select ss.id "SSID"
, ss.Date_of_Success__c
, convert(varchar(1000),ss.So_What__c) "SoWhat"
, "SoWhatLong" = Case when LEN(ss.so_what__c) &gt; 1000 then '*' else '' end
, convert(varchar(1000),ss.Success_Criteria__c) "Criteria"
, "CriteriaLong" = case when LEN(convert(varchar(1050),ss.Success_Criteria__c)) &gt; 1000 then '*' else '' end
, convert(varchar(1000),ss.Success_Description__c) "Description"
, "DescLong" = Case when LEN(convert(varchar(1050),ss.Success_Description__c)) &gt; 1000 then '*' else '' end
, o.Id "OppId"
, o.Opp_Name__c
, o.Cluster__c
, o.NT_Staff_Engaged__c
, acc.Name "Org"
from NT_Opp_Success_Story__c ss
inner join NT_Opportunity__c o on ss.NT_Opportunity__c = o.id
inner join NT_Opp_Participant__c p on o.Id = p.NT_Opportunity__c
inner join Account acc on p.Organization__c = acc.id
where acc.Id = @Org</CommandText>
