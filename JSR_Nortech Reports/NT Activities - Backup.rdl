  <DataSets>
    <DataSet Name="Activities">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subject">
          <DataField>subject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RelatedTo">
          <DataField>RelatedTo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="whatid">
          <DataField>whatid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="activitydate">
          <DataField>activitydate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select t.id
, u.name
, t.status
, t.subject
, "RelatedTo" = case
  when substring(t.whatid, 1,4) = 'a0i4' then 'NTOpp: '+nto.name
  when substring(t.whatid, 1,4) = '0014' then 'Org: '+acc2.name
  when t.whatid is not null then 'Unknown'
  end
, t.whatid
, t.activitydate
from task t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
and t.activitydate between @Start and @End
UNION
select t.id
, u.name
, "Status" = case
  when t.activitydate &lt; getdate() then 'Past Calendar Event'
  when t.activitydate = getdate() then 'Current Calendar Event'
  when t.activitydate &gt; getdate() then 'Future Calendar Event'
  end
, t.subject
, "RelatedTo" = case
  when substring(t.whatid, 1,4) = 'a0i4' then 'NTOpp: '+nto.name
  when substring(t.whatid, 1,4) = '0014' then 'Org: '+acc2.name
  when t.whatid is not null then 'Unknown'
  end
, t.whatid
, t.activitydate
from event t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
and t.activitydate between @Start and @End</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Start">
            <Value>=Parameters!Start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End">
            <Value>=Parameters!End.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DateParam">
      <Fields>
        <Field Name="MinDate">
          <DataField>MinDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="MaxDate">
          <DataField>MaxDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select min(a.activitydate) "MinDate", max(a.activitydate) "MaxDate"
from (select t.id, t.activitydate
from task t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
UNION
select t.id,  t.activitydate
from event t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%') a</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
