  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>SELECT     SUM(CASE WHEN Pipeline_Status__c = 'Incubating' THEN 1 ELSE 0 END) AS IncCount, SUM(CASE WHEN Pipeline_Status__c = 'Demonstrating' THEN 1 ELSE 0 END) 
                      AS DemCount, SUM(CASE WHEN Pipeline_Status__c = 'Market Entry' THEN 1 ELSE 0 END) AS MECount, 
                      SUM(CASE WHEN Pipeline_Status__c = 'Growth' THEN 1 ELSE 0 END) AS GrowCount, SUM(CASE WHEN Pipeline_Status__c = 'Impact' THEN 1 ELSE 0 END) 
                      AS ImpCount
FROM         NT_Opportunity__c</CommandText>
      </Query>
      <Fields>
        <Field Name="IncCount">
          <DataField>IncCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DemCount">
          <DataField>DemCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MECount">
          <DataField>MECount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GrowCount">
          <DataField>GrowCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ImpCount">
          <DataField>ImpCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
