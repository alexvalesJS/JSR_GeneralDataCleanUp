    <DataSet Name="DateParam">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select min(a.activitydate) "MinDate", max(a.activitydate) "MaxDate"
from (select t.id, t.activitydate
from task t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
UNION
select t.id,  t.activitydate
from event t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%') a</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="MinDate">
          <DataField>MinDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="MaxDate">
          <DataField>MaxDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
