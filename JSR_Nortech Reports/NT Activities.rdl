  <DataSets>
    <DataSet Name="Activities">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start">
            <Value>=Parameters!Start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End">
            <Value>=Parameters!End.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select t.id
, u.name
, t.status
, t.subject
, "RelatedTo" = case
  when substring(t.whatid, 1,4) = 'a0i4' then 'NTOpp: '+nto.name
  when substring(t.whatid, 1,4) = '0014' then 'Org: '+acc2.name
  when t.whatid is not null then 'Unknown'
  end
, t.whatid
, t.activitydate
from task t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
and t.activitydate between @Start and @End
UNION
select t.id
, u.name
, "Status" = case
  when t.activitydate &lt; getdate() then 'Past Calendar Event'
  when t.activitydate = getdate() then 'Current Calendar Event'
  when t.activitydate &gt; getdate() then 'Future Calendar Event'
  end
, t.subject
, "RelatedTo" = case
  when substring(t.whatid, 1,4) = 'a0i4' then 'NTOpp: '+nto.name
  when substring(t.whatid, 1,4) = '0014' then 'Org: '+acc2.name
  when t.whatid is not null then 'Unknown'
  end
, t.whatid
, t.activitydate
from event t
inner join [user] u on t.ownerid = u.id
inner join contact c on u.user_to_contact_lookup__c = c.id
inner join account acc on c.accountid = acc.id
left outer join nt_opportunity__c nto on t.whatid = nto.id
left outer join account acc2 on t.whatid = acc2.id
where acc.name like 'Nortech%'
and t.activitydate between @Start and @End</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
