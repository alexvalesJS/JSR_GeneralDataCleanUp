      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandText>select a.TR_ID
, a.TR_Name
, a.Actual_Amount__c
, a.Actual_Disbursement_Date__c
, a.RptDate
, a.OrgProvFunds
, a.OrgRecFunds
, a.Cluster__c
, a.NTCluster
from (select tr.Id "TR_ID"
, tr.Name "TR_Name"
, tr.Actual_Amount__c
, tr.Actual_Disbursement_Date__c
, "RptDate" = case
  when month(tr.Actual_Disbursement_Date__c) = 1 then 'Jan-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 2 then 'Feb-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 3 then 'Mar-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 4 then 'Apr-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 5 then 'May-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 6 then 'Jun-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 7 then 'Jul-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 8 then 'Aug-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 9 then 'Sep-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 10 then 'Oct-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 11 then 'Nov-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 12 then 'Dec-' + convert(varchar,year(tr.actual_disbursement_date__c))
end
, opf.Name "OrgProvFunds"
, orf.Name "OrgRecFunds"
, convert(varchar,orf.Cluster__c) "Cluster__c"
, 'FlexMatters' "NTCluster"
from tranche_release__c tr
inner join company_funding_round__c cfr on tr.Funding_Round__c = cfr.Id
inner join account orf on cfr.Organization_Receiving_Funds__c = orf.Id
inner join account opf on tr.Organization_Providing_Funds__c = opf.Id
where tr.Funding_Status__c like '%Closed'
and tr.Actual_Amount__c &gt; 0
and tr.Actual_disbursement_Date__c is not null
and coalesce(cfr.Funding_Round__c ,'Exit') &lt;&gt; 'Exit'
and convert(varchar,orf.cluster__c) like '%FlexMatters%'
and tr.Actual_disbursement_date__c between @Start and @End
UNION
select tr.Id "TR_ID"
, tr.Name "TR_Name"
, tr.Actual_Amount__c
, tr.Actual_Disbursement_Date__c
, "RptDate" = case
  when month(tr.Actual_Disbursement_Date__c) = 1 then 'Jan-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 2 then 'Feb-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 3 then 'Mar-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 4 then 'Apr-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 5 then 'May-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 6 then 'Jun-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 7 then 'Jul-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 8 then 'Aug-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 9 then 'Sep-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 10 then 'Oct-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 11 then 'Nov-' + convert(varchar,year(tr.actual_disbursement_date__c))
  when month(tr.Actual_Disbursement_Date__c) = 12 then 'Dec-' + convert(varchar,year(tr.actual_disbursement_date__c))
end
, opf.Name "OrgProvFunds"
, orf.Name "OrgRecFunds"
, convert(varchar,orf.Cluster__c) "Cluster__c"
, 'Energy Enterprise' "NTCluster"
from tranche_release__c tr
inner join company_funding_round__c cfr on tr.Funding_Round__c = cfr.Id
inner join account orf on cfr.Organization_Receiving_Funds__c = orf.Id
inner join account opf on tr.Organization_Providing_Funds__c = opf.Id
where tr.Funding_Status__c like '%Closed'
and tr.Actual_Amount__c &gt; 0
and tr.Actual_disbursement_Date__c is not null
and coalesce(cfr.Funding_Round__c ,'Exit') &lt;&gt; 'Exit'
and convert(varchar,orf.cluster__c) like '%Energy Enterprise%'
and tr.Actual_disbursement_date__c between @Start and @End) a
where a.NTCluster in (@Cluster)</CommandText>
