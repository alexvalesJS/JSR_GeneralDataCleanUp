        </Field>
        <Field Name="ESP_Qualified_Deal_Flow_Date__c">
          <DataField>ESP_Qualified_Deal_Flow_Date__c</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreatedDate">
          <DataField>CreatedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreatedBy">
          <DataField>CreatedBy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="referralGroup">
          <DataField>referralGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select  acc.name as Related_Organization2
, org.name as OrgName
, cs.Name as CS_Name
, 'https://na2.salesforce.com/' + cs.id as CS_URL
, cs.[ESP_Qualified_Deal_Flow_Date__c]
	, cc.CreatedDate
	, u.name as CreatedBy
	, Case when acc.name like '%sbdc%' then 'SBDC' 
			when acc.name like '%SCORE%' then 'SCORE' 
			when acc.name like '%Tech Spout%' then 'Tech Spout' 
			when acc.name like '%Hispanic Business Associations%' then 'Hispanic Business Associations' 
			else 'Other' End as referralGroup
from Contact_Connection__c cc
INNER JOIN [dbo].[Client_Services__c] cs on cc.client_Services__c = cs.id
	inner join RecordType rt on rt.Id = cc.RecordTypeId
	inner join account acc on acc.id = cc.Related_Organization_2__c
	left join account org on cs.[Organization__c] = org.id
	LEFT JOIN [User] u on cc.[CreatedById] = u.id
	 LEFT JOIN (SELECT cc.Related_Organization_2__c, COUNT(cc.Related_Organization_2__c) as ct
					from Contact_Connection__c cc
					INNER JOIN [dbo].[Client_Services__c] cs on cc.client_Services__c = cs.id
					inner join RecordType rt on rt.Id = cc.RecordTypeId
					where rt.Name = 'referral for services'
						and cc.CreatedDate between @startdate and @enddate
					GROUP BY cc.Related_Organization_2__c				
				) ct on cc.Related_Organization_2__c = ct.Related_Organization_2__c
				where rt.Name = 'referral for services'
			and cc.CreatedDate between @startdate and @enddate
ORDER BY ct.ct DESC, org.name, cc.CreatedDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!enddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
