        </Field>
        <Field Name="Minority_Controlled__c">
          <DataField>Minority_Controlled__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Minority_Led__c">
          <DataField>Minority_Led__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Minority_Owned__c">
          <DataField>Minority_Owned__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ethnicity__c">
          <DataField>Ethnicity__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SFDown</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select 
  'https://na2.salesforce.com/' + acc.ID as URLlink
  , acc.Id OrgID
, acc.name as OrgName
, acc.Minority_Controlled__c 
, acc.Minority_Led__c 
, acc.Minority_Owned__c
, acc.Ethnicity__c
from client_services__c cs
join Account acc on cs.Organization__c = acc.Id
	inner join RecordType rt on cs.RecordTypeId = rt.Id
where not cs.Network_Services_Client_Date__c is null
	and rt.Name = 'NEO OTF ESP'
	and (((acc.Minority_Controlled__c = 'true' or acc.Minority_Led__c = 'true' or acc.Minority_Owned__c= 'true') and (acc.Ethnicity__c is null or acc.Ethnicity__c = 'Caucasian')))
	OR
	((NOT acc.Ethnicity__c IS NULL AND acc.Ethnicity__c &lt;&gt; 'Caucasian') AND (acc.Minority_Controlled__c = 'false' AND acc.Minority_Led__c = 'false' AND acc.Minority_Owned__c = 'false'))
	ORDER BY acc.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
