  <DataSets>
    <DataSet Name="TechSpinOff">
      <Query>
        <DataSourceName>SFDown</DataSourceName>
        <CommandText>select distinct
 conn.id as ConnID
  ,conn.Type__c
 ,conn.Related_Organization_1__c as  Related_Organization1
 , acc1.Name as RelOrg1name
,  conn.Related_Organization_2__c as  Related_Organization2
 , acc2.Name as RelOrg2name

from Contact_Connection__c conn
       join Account acc1 on conn.Related_Organization_1__c=acc1.Id
       join Account acc2 on conn.Related_Organization_2__c=acc2.id
       left join Client_Services__c csr1 on acc1.Id=csr1.Organization__c
       left join Client_Services__c csr2 on acc2.Id=csr2.Organization__c


WHERE     
(conn.Type__c = 'Technology Spinoff')
and (not csr1.[Organization__c] is null or not csr2.[Organization__c] is null)
order by acc1.Name, acc2.Name

</CommandText>
      </Query>
      <Fields>
        <Field Name="ConnID">
          <DataField>ConnID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Type__c">
          <DataField>Type__c</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Related_Organization1">
