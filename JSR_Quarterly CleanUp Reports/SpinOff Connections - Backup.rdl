        </Field>
        <Field Name="Related_OrgName_2">
          <DataField>Related_OrgName_2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SFDown</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select distinct
 conn.id as ConnID
  ,conn.Type__c
 ,conn.Related_Organization_1__c as  Related_OrgID_1
 , acc1.Name as Related_OrgName_1
,  conn.Related_Organization_2__c as  Related_OrgID_2
 , acc2.Name as Related_OrgName_2

from Contact_Connection__c conn
       join Account acc1 on conn.Related_Organization_1__c=acc1.Id
       join Account acc2 on conn.Related_Organization_2__c=acc2.id
       left join Client_Services__c csr1 on acc1.Id=csr1.Organization__c
       left join Client_Services__c csr2 on acc2.Id=csr2.Organization__c


WHERE     
(conn.Type__c = 'Technology Spinoff')
and (not csr1.[Organization__c] is null or not csr2.[Organization__c] is null)
order by acc1.Name, acc2.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
